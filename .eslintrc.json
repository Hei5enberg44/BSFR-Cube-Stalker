{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prefer-const": "error",
        "quotes": ["error", "single"],
        "semi": ["error", "never"]
    }
}
